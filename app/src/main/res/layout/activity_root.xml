<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.RootActivity"
        android:id="@+id/root_layout_frame">

    <fragment
            android:name="androidx.navigation.fragment.NavHostFragment"
            app:navGraph="@navigation/nav_graph"
            app:defaultNavHost="true"
            tools:layout_editor_absoluteY="8dp"
            tools:layout_editor_absoluteX="8dp"
            android:id="@+id/fragment2"
            app:layout_constraintTop_toTopOf="@id/root_layout_frame"
            app:layout_constraintRight_toRightOf="@id/root_layout_frame"
            app:layout_constraintLeft_toLeftOf="@id/root_layout_frame"
            app:layout_constraintBottom_toBottomOf="@id/root_layout_frame"
            android:layout_height="0dp"
            android:layout_width="0dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
